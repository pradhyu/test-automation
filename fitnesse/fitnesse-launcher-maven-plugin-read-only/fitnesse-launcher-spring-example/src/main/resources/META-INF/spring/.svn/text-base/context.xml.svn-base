<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="buyMilk1" class="fitnesse.slim.test.ShouldIBuyMilk">
        <property name="cashInWallet" value="0"/>
        <property name="creditCard" value="no"/>
        <property name="pintsOfMilkRemaining" value="0"/>
    </bean>
    
    <bean id="buyMilk2" class="fitnesse.slim.test.ShouldIBuyMilk">
        <property name="cashInWallet" value="10"/>
        <property name="creditCard" value="no"/>
        <property name="pintsOfMilkRemaining" value="0"/>
    </bean>
</beans>